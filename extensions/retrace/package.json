{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "retrace",
  "title": "Retrace Quick Actions",
  "description": "Open Retrace timeline and search using deeplinks",
  "icon": "retrace.png",
  "author": "haseab_",
  "contributors": [
    "pernielsentikaer"
  ],
  "categories": [
    "Applications"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "go-to-retrace-timestamp",
      "title": "Go to Retrace Timestamp",
      "subtitle": "Retrace",
      "description": "Enter a time in natural language (e.g. '2nd day of march', '3 minutes ago', 'Feb 1 2am', 'yesterday 8am')",
      "mode": "no-view",
      "arguments": [
        {
          "name": "time",
          "placeholder": "e.g. 3 minutes ago",
          "type": "text",
          "required": true
        }
      ]
    },
    {
      "name": "go-to-last-retrace-timestamp",
      "title": "Go to Last Retrace Timestamp",
      "subtitle": "Retrace",
      "description": "Launches Previous Retrace Timestamp",
      "mode": "no-view"
    },
    {
      "name": "search-retrace",
      "title": "Search Retrace",
      "subtitle": "Retrace",
      "description": "Search in Retrace using retrace://search?q=...",
      "mode": "no-view",
      "arguments": [
        {
          "name": "query",
          "placeholder": "e.g. oauth callback error",
          "type": "text",
          "required": true
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.104.0",
    "@raycast/utils": "^2.2.2",
    "chrono-node": "^2.8.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.1.1",
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "eslint": "^9.22.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  },
  "scripts": {
    "build": "ray build",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "prepublishOnly": "echo \"Use npm run publish to publish to Raycast Store.\" && exit 1",
    "publish": "npx @raycast/api@latest publish"
  },
  "platforms": [
    "macOS"
  ]
}
