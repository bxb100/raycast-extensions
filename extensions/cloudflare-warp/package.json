{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "cloudflare-warp",
  "title": "Cloudflare WARP",
  "description": "Manage Cloudflare WARP",
  "icon": "extension-icon.png",
  "author": "anandubajith",
  "contributors": [
    "ezzylan"
  ],
  "categories": [
    "Applications"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "toggle-connection",
      "title": "Toggle Connection",
      "description": "Toggle WARP connection",
      "mode": "no-view"
    },
    {
      "name": "switch-virtual-network",
      "title": "Switch Virtual Network",
      "description": "Switch between available virtual networks",
      "mode": "view"
    },
    {
      "name": "switch-mdm-profile",
      "title": "Switch MDM Profile",
      "description": "Switch between available MDM profiles",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "wrapCliPath",
      "type": "textfield",
      "required": false,
      "default": "/Applications/Cloudflare WARP.app/Contents/Resources/warp-cli",
      "title": "wrap-cli executable path",
      "description": "Set this if your wrap-cli executable is in a non-standard location"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.104.6"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.1.1",
    "@types/node": "25.2.3",
    "@types/react": "19.2.14",
    "eslint": "^10.0.0",
    "eslint-plugin-unicorn": "^63.0.0",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  },
  "platforms": [
    "macOS"
  ]
}
